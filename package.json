{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@terrestris/react-geo": "^13.0.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.3.1",
    "body-parser": "^1.19.0",
    "ol": "^6.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "twitter": "^1.7.1"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server.js",
